{
  "name": "@cyborgtests/test",
  "version": "0.0.8",
  "description": "Powerfull extension for Playwright, that allows you to include manual verification steps in your automated test flow",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": {
    "url": "https://github.com/CyborgTests/cyborg-test"
  },
  "scripts": {
    "build:lib": "vite build",
    "build:controlPanel": "vite build --config vite.config.app.ts",
    "build:testBuilder": "vite build --config vite.config.testBuilder.ts",
    "prepare": "npm run build:controlPanel && npm run build:lib && npm run build:testBuilder",
    "dev": "vite --config vite.config.app.ts",
    "dev:testBuilder": "vite --config vite.config.testBuilder.ts",
    "preview": "vite preview",
    "postinstall": "node src/postinstall/generate-uuid.js"
  },
  "keywords": [
    "testing",
    "cyborg tests",
    "manual step",
    "playwright"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@playwright/test": "^1.52.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.4",
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.11",
    "vite-plugin-singlefile": "^2.2.0"
  },
  "files": [
    "dist",
    "control-panel-build",
    "test-builder-build",
    "README.md",
    "LICENSE",
    "package.json",
    "package-lock.json",
    "src"
  ],
  "dependencies": {
    "os": "^0.1.2"
  }
}
